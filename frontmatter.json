{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "weight",
          "name": "weight",
          "type": "number"
        },
        {
          "title": "comments",
          "name": "comments",
          "type": "boolean"
        },
        {
          "title": "toc",
          "name": "toc",
          "type": "boolean"
        },
        {
          "title": "cascade",
          "name": "cascade",
          "type": "fields",
          "fields": [
            {
              "title": "type",
              "name": "type",
              "type": "string"
            }
          ]
        },
        {
          "title": "breadcrumbs",
          "name": "breadcrumbs",
          "type": "boolean"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        }
      ]
    }
  ],
  "frontMatter.content.pageFolders": [
    {
      "title": "docs",
      "path": "[[workspace]]/content/docs"
    },
    {
      "title": "content",
      "path": "[[workspace]]/content"
    },
    {
      "title": "pc-specs",
      "path": "[[workspace]]"
    }
  ]
}